CREATE TABLE TUser (
    ID INTEGER PRIMARY KEY AUTO_INCREMENT,
    NAME VARCHAR(100) NOT NULL,
    ADDRESS VARCHAR(100) NOT NULL,
    PHNUMBER VARCHAR(100) NOT NULL
);

DELIMITER //
mysql> CREATE PROCEDURE insertRowTUser(IN nm varchar(100), IN addr varchar(100), IN phn varchar(100))
    BEGIN
    INSERT INTO TUser(NAME, ADDRESS, PHNUMBER) VALUES(nm, addr, phn);
    END //
DELIMITER ;


DELIMITER //
CREATE PROCEDURE getRowTUser(IN number INTEGER)
BEGIN
SELECT * FROM TUser WHERE ID = number;
END //
DELIMITER ;
